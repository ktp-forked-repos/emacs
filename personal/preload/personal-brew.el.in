;;; Code:

(add-to-list
 'load-path
 "@BREW_SITE_LISP_PATH@")

;; Use `declare-function' to silence Flycheck and the compiler.
(declare-function
 prelude-add-subfolders-to-load-path
 "@BREW_SITE_LISP_PATH@")
@GETTEXT_COMMANDS@
(provide 'personal-brew)

